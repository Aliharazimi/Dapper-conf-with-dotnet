@{
    ViewData["Title"] = "Home Page";
}
@model User
<form method="POST" enctype="multipart/form-data">
    <div class="form-group">
        <label for="exampleInputEmail1">username</label>
        <input type="text" class="form-control" asp-for="UserName"  id="exampleInputEmail1" aria-describedby="" placeholder="username">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1" asp-for="Password" placeholder="Password">
    </div>

    <button type="submit" class="btn btn-primary mt-5">Submit</button>
</form>